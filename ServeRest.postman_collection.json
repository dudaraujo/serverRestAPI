			"name": "Geração de Massa",
			"item": [
				{
					"name": "Buscar nova pessoa",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"\r",
									"//Capturar nome, email e senha \r",
									"\r",
									"pm.test('Capturar nome, email e senha', function () {\r",
									"    pm.globals.set('userName', pm.response.json().results[0].name.first)\r",
									"    pm.globals.set('userMail', pm.response.json().results[0].email)\r",
									"    pm.globals.set('userPassword', pm.response.json().results[0].login.password)\r",
									"})"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "https://randomuser.me/api/",
							"protocol": "https",
							"host": [
								"randomuser",
								"me"
							],
							"path": [
								"api",
								""
							]
						}
					},
					"response": []
				}
			]
